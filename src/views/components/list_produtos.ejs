


  <% if (typeof produtos !== 'undefined') { %>
  <% if (produtos.length === 0) { %>
    <p>Não há produtos disponíveis no momento</p>
  <% } else { %>
    <% for (let produto of produtos) { %>
      <%- include('produto', {imagem:produto.foto,nome:produto.nome,valor:produto.valor,descricao:produto.descricao,categoria:produto.categoria,tipo:produto.tipo,anunciante:produto.userName,telefone:produto.userNumber,cidade:produto.userCity,UF:produto.userUF}) %>
    <% } %>
  <% }} %>
